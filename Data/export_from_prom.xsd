<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="yml_catalog">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="shop" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="name" type="xs:string" minOccurs="0" />
              <xs:element name="company" type="xs:string" minOccurs="0" />
              <xs:element name="url" type="xs:string" minOccurs="0" />
              <xs:element name="currencies" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="currency" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="id" type="xs:string" />
                        <xs:attribute name="rate" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="categories" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="category" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="category_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="id" type="xs:string" />
                            <xs:attribute name="parentId" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="offers" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="offer" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="url" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
                          <xs:element name="oldprice" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
                          <xs:element name="price" type="xs:string" minOccurs="0" msdata:Ordinal="2" />
                          <xs:element name="currencyId" type="xs:string" minOccurs="0" msdata:Ordinal="3" />
                          <xs:element name="categoryId" type="xs:string" minOccurs="0" msdata:Ordinal="4" />
                          <xs:element name="pickup" type="xs:string" minOccurs="0" msdata:Ordinal="6" />
                          <xs:element name="delivery" type="xs:string" minOccurs="0" msdata:Ordinal="7" />
                          <xs:element name="name" type="xs:string" minOccurs="0" msdata:Ordinal="8" />
                          <xs:element name="name_ua" type="xs:string" minOccurs="0" msdata:Ordinal="9" />
                          <xs:element name="vendor" type="xs:string" minOccurs="0" msdata:Ordinal="10" />
                          <xs:element name="vendorCode" type="xs:string" minOccurs="0" msdata:Ordinal="11" />
                          <xs:element name="description" type="xs:string" minOccurs="0" msdata:Ordinal="12" />
                          <xs:element name="description_ua" type="xs:string" minOccurs="0" msdata:Ordinal="13" />
                          <xs:element name="sales_notes" type="xs:string" minOccurs="0" msdata:Ordinal="14" />
                          <xs:element name="picture" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:simpleContent msdata:ColumnName="picture_Text" msdata:Ordinal="0">
                                <xs:extension base="xs:string">
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="param" nillable="true" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:simpleContent msdata:ColumnName="param_Text" msdata:Ordinal="2">
                                <xs:extension base="xs:string">
                                  <xs:attribute name="name" type="xs:string" />
                                  <xs:attribute name="unit" type="xs:string" />
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:string" />
                        <xs:attribute name="available" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="date" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="yml_catalog" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>