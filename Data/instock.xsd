<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="Владелец">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Ид" type="xs:string" minOccurs="0" />
        <xs:element name="Наименование" type="xs:string" minOccurs="0" />
        <xs:element name="ОфициальноеНаименование" type="xs:string" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Группы">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Ид" type="xs:string" minOccurs="0" />
        <xs:element name="Группа" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Ид" type="xs:string" minOccurs="0" />
              <xs:element name="Наименование" type="xs:string" minOccurs="0" />
              <xs:element name="Родитель" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="КоммерческаяИнформация">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Классификатор" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Ид" type="xs:string" minOccurs="0" />
              <xs:element name="Наименование" type="xs:string" minOccurs="0" />
              <xs:element ref="Владелец" minOccurs="0" maxOccurs="unbounded" />
              <xs:element ref="Группы" minOccurs="0" maxOccurs="unbounded" />
              <xs:element name="Свойства" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Свойство" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Ид" type="xs:string" minOccurs="0" />
                          <xs:element name="Наименование" type="xs:string" minOccurs="0" />
                          <xs:element name="Обязательное" type="xs:string" minOccurs="0" />
                          <xs:element name="Множественное" type="xs:string" minOccurs="0" />
                          <xs:element name="ТипЗначений" type="xs:string" minOccurs="0" />
                          <xs:element name="ДляТоваров" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="ПакетПредложений" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Ид" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
              <xs:element name="ИдКлассификатора" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
              <xs:element name="Наименование" type="xs:string" minOccurs="0" msdata:Ordinal="2" />
              <xs:element ref="Владелец" minOccurs="0" maxOccurs="unbounded" />
              <xs:element name="ТипыЦен" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ТипЦены" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Ид" type="xs:string" minOccurs="0" />
                          <xs:element name="Наименование" type="xs:string" minOccurs="0" />
                          <xs:element name="ИДЦенаСайт" type="xs:string" minOccurs="0" />
                          <xs:element name="Валюта" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Предложения" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Предложение" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Ид" type="xs:string" minOccurs="0" />
                          <xs:element name="Цены" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Цена" minOccurs="0" maxOccurs="unbounded">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="Представление" type="xs:string" minOccurs="0" />
                                      <xs:element name="ИдТипаЦены" type="xs:string" minOccurs="0" />
                                      <xs:element name="ИДЦенаСайт" type="xs:string" minOccurs="0" />
                                      <xs:element name="ЦенаЗаЕдиницу" type="xs:string" minOccurs="0" />
                                      <xs:element name="Валюта" type="xs:string" minOccurs="0" />
                                      <xs:element name="Единица" type="xs:string" minOccurs="0" />
                                      <xs:element name="Коэффициент" type="xs:string" minOccurs="0" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Товары" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Товар" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Ид" type="xs:string" minOccurs="0" />
                          <xs:element name="Артикул" type="xs:string" minOccurs="0" />
                          <xs:element name="КодТовара" type="xs:string" minOccurs="0" />
                          <xs:element name="Наименование" type="xs:string" minOccurs="0" />
                          <xs:element name="ПолноеНаименование" type="xs:string" minOccurs="0" />
                          <xs:element name="БазоваяЕдиница" type="xs:string" minOccurs="0" />
                          <xs:element name="Описание" type="xs:string" minOccurs="0" />
                          <xs:element ref="Группы" minOccurs="0" maxOccurs="unbounded" />
                          <xs:element name="ЗначенияСвойств" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="ЗначенияСвойства" minOccurs="0" maxOccurs="unbounded">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="Ид" type="xs:string" minOccurs="0" />
                                      <xs:element name="Наименование" type="xs:string" minOccurs="0" />
                                      <xs:element name="Значение" type="xs:string" minOccurs="0" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="СодержитТолькоИзменения" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="ВерсияСхемы" type="xs:string" />
      <xs:attribute name="ДатаФормирования" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Владелец" />
        <xs:element ref="Группы" />
        <xs:element ref="КоммерческаяИнформация" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>